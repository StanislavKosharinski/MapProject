{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -R -f dist",
    "build": "npm run clean && webpack",
    "compress": "wepback -p",
    "start-dev-compressed": "npm run build && npm run compress && webpack-dev-server --port 3000 --hot",
    "start-prod-compressed": "npm run build ** npm run compress && webpack-dev-server --port 3000 --host 0.0.0.0",
    "start-build": "npm run build && webpack-dev-server --port 3000 --hot",
    "start": "webpack-dev-server --port 3000"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "angular2-tabs": "0.1.0-beta.6",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "ts-loader": "^0.7.2"
  }
}
